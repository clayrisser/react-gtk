{
  "name": "react-gtk",
  "version": "0.1.0-rc.0",
  "description": "cross-platform react native desktop applications",
  "private": true,
  "sideEffects": false,
  "keywords": [
    "react-gtk",
    "react",
    "gtk"
  ],
  "homepage": "https://github.com/silicon-hills/react-gtk",
  "bugs": {
    "url": "https://github.com/silicon-hills/react-gtk/issues",
    "email": "support@siliconhills.dev"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Clay Risser",
    "email": "email@clayrisser.com",
    "url": "https://clayrisser.com"
  },
  "files": [
    "es",
    "lib"
  ],
  "module": "es/index.js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/silicon-hills/react-gtk"
  },
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "coverage": "make -s coverage",
    "format": "make -s format",
    "inc": "make -s inc",
    "lint": "make -s lint",
    "make": "make",
    "prepare": "make -s prepare",
    "purge": "make -s purge",
    "report": "make -s report",
    "spellcheck": "make -s spellcheck",
    "start": "make -s start",
    "test": "make -s test",
    "test:ui": "make test-ui",
    "test:watch": "make -s test-watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "core-js": "^3.15.2"
  },
  "peerDependencies": {
    "node-gtk": "^0.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cspell": "^5.6.6",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-spellcheck": "^0.0.19",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "lockfile-lint": "^4.6.2",
    "majestic": "^1.8.1",
    "node-gtk": "^0.9.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "xmldom": "^0.6.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "es",
    "lib"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/es/",
      "<rootDir>/lib/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  }
}
