{
  "name": "@react-gtk/core",
  "version": "4.0.0",
  "description": "react gtk core",
  "sideEffects": false,
  "homepage": "https://gitlab.com/bitspur/community/react-gtk",
  "bugs": {
    "url": "https://gitlab.com/bitspur/community/react-gtk/issues",
    "email": "support@bitspur.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/bitspur/community/react-gtk"
  },
  "license": "Apache-2.0",
  "author": "BitSpur <email@clayrisser.com> (https://clayrisser.com)",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "bridge",
    "desktop",
    "gnome",
    "gtk",
    "native",
    "pc",
    "react",
    "react-native",
    "render"
  ],
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "\\.(spec|test)\\.[cm]?[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "testPathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "jestSonar": {
    "reportPath": ".mkpm/mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "dependencies": {
    "@girs/node-gdk-4.0": "^4.0.0-3.2.6",
    "@girs/node-glib-2.0": "^2.78.0-3.2.6",
    "@girs/node-gobject-2.0": "^2.78.0-3.2.6",
    "@girs/node-graphene-1.0": "^1.0.0-3.2.6",
    "@girs/node-gtk-4.0": "^4.12.3-3.2.6",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "node-gtk": "^0.12.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-native": "^0.73.1",
    "react-reconciler": "^0.29.0",
    "yoga-layout": "^2.0.1"
  },
  "devDependencies": {
    "@react-gtk/generate": "workspace:^",
    "@swc/core": "^1.3.99",
    "@types/lodash.debounce": "^4",
    "@types/lodash.kebabcase": "^4",
    "@types/lodash.snakecase": "^4",
    "@types/node": "^20.10.0",
    "@types/prop-types": "^15.7.11",
    "@types/react": "^18.2.39",
    "@types/react-reconciler": "^0.28.8",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
